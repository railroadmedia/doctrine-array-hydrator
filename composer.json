{
  "name": "railroad/doctrine-array-hydrator",
  "description": " An array to entity hydrator for Doctrine 2 entities ",
  "keywords": ["php", "doctrine", "hydrator", "array"],
  "homepage": "https://github.com/railroadmedia/doctrine-array-hydrator",
  "license": "MIT",
  "authors": [
    {
      "name": "pmill",
      "email": "dev.pmill@gmail.com"
    },
    {
      "name": "Caleb",
      "email": "caleb@drumeo.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "doctrine/orm": "2.12.*",
    "symfony/cache": "^6.0",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {"Railroad\\DoctrineArrayHydrator\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\DoctrineArrayHydrator\\Tests\\": "tests/"
    }
  }
}
